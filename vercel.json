{
    "version": 2,
    "builds": [
        {
            "src": "api/index.php",
            "use": "vercel-php@0.7.2",
            "config": {
                "includeFiles": "{database/database.sqlite,storage/**,bootstrap/cache/**}"
            }
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "public/build/$1",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/(css|js|images|favicon.ico|robots.txt)/(.*)",
            "dest": "public/$1/$2",
            "headers": {
                "Cache-Control": "public, max-age=31536000"
            }
        },
        {
            "src": "/favicon.ico",
            "dest": "public/favicon.ico"
        },
        {
            "src": "/robots.txt",
            "dest": "public/robots.txt"
        },
        {
            "src": "/(.*)",
            "dest": "api/index.php",
            "headers": {
                "Strict-Transport-Security": "max-age=31536000; includeSubDomains",
                "X-Frame-Options": "SAMEORIGIN",
                "X-Content-Type-Options": "nosniff"
            }
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_KEY": "base64:6UpR73Mmd0mDN2J5ITZp2hK6QVBt+eFH8uNQ0Yqb/nU=",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",
        "SESSION_LIFETIME": "120",
        "SESSION_ENCRYPT": "false",
        "SESSION_PATH": "/",
        "SESSION_DOMAIN": "vala-project-agadir.vercel.app",
        "SESSION_SECURE_COOKIE": "true",
        "SESSION_SAME_SITE": "lax",
        "CACHE_STORE": "array",
        "QUEUE_CONNECTION": "sync",
        "VIEW_COMPILED_PATH": "/tmp",
        "LOG_LEVEL": "debug",
        "ASSET_URL": "https://vala-project-agadir.vercel.app",
        "APP_URL": "https://vala-project-agadir.vercel.app",
        "FORCE_HTTPS": "true",
        "APP_FORCE_HTTPS": "true",
        "DB_CONNECTION": "sqlite",
        "DB_DATABASE": "database/database.sqlite",
        "GOOGLE_REDIRECT_URI": "https://vala-project-agadir.vercel.app/auth/google-callback",
        "MAIL_MAILER": "smtp",
        "MAIL_FROM_ADDRESS": "noreply@vala-project-agadir.vercel.app",
        "MAIL_FROM_NAME": "Best Agadir Activities"
    }
}